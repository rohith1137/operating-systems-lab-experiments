#include <stdio.h>

int main() {
    int q[20], n, head, i;
    int seek = 0;

    printf("Enter number of requests: ");
    scanf("%d", &n);

    printf("Enter disk requests:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &q[i]);

    printf("Enter initial head position: ");
    scanf("%d", &head);

    for (i = 0; i < n; i++) {
        seek += abs(q[i] - head);
        head = q[i];
    }

    printf("Total seek time = %d\n", seek);
    return 0;
}
