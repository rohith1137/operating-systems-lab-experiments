#include <stdio.h>

int main() {
    int frames[3] = {-1, -1, -1};
    int pages[10], i, j, n, index = 0;

    printf("Enter number of pages: ");
    scanf("%d", &n);

    printf("Enter page reference string:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &pages[i]);

    for (i = 0; i < n; i++) {
        frames[index] = pages[i];
        index = (index + 1) % 3;

        for (j = 0; j < 3; j++)
            printf("%d ", frames[j]);
        printf("\n");
    }
    return 0;
}
